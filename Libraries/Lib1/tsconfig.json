{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "rootDir": "./",
    "outDir": "./src-gen",
    "baseUrl": "./",
    "paths": {
      "src/my/libraries/lib1/*": ["./src/my/libraries/lib1/*"]
    }
  },
  "include": ["./src/**/*"]
}
